<%= link_to 'Home', billboards_path %>
<%= link_to 'All Artists', artists_path %> 
<h1><%= @billboard.name %></h1>
<table>
<thead>
  <th>Artist</th>
  <th>Song</th>
  <th>Album</th> 
</thead>
<% @artists.each do |artist| %>
  <% artist.songs.each do |song| %>
  <tr>
    <td><%=link_to artist.name, artist_path(artist) %></td> 
    <td><%=song.title %></td>
    <td><%=song.album %></td> 
  </tr>
  <% end %>
<% end %>

</table> 


<%= link_to 'Edit', edit_billboard_path(@billboard) %>
<%= link_to 'Delete', billboard_path(@billboard), method: :delete %>
